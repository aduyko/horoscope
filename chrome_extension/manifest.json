{
    "manifest_version": 2,

    "name": "Horoscope",
    "description": "A chrome extension for showing you your daily horoscope when you open a new tab",
    "version": "0.1",
    "permissions": [
        "storage"
    ],
    "chrome_url_overrides" : {
        "newtab": "app/horoscope.html"
    },
    "options_ui": {
        "page": "options/options.html",
        "chrome_style": false,
        "open_in_tab": true
    },
    "options_page": "options/options.html",
    "web_accessible_resources": [
        "app/*",
        "options/*",
        "images/*"
    ]
}